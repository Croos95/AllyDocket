VARIABLES{
  ENTERO #A = 10;
  ENTERO #B = 5;
  ENTERO #C = 6;
  ENTERO #F = 8;
  TEXTO #A = ADSA4;
}

PROCESOS {
  SUMA [#A ,#B] = #C;
  SI(#a << #B){
    MULTIPLICACION [#A ,#B] = #C;
    SI(#B << #C){
      SUMA [#A,#B] = #C;	
    }
  }
  MIENTRAS(#A<<#B){
    SUMA [#C,#F] = #C;
    SI (#C << #F){
      DIVISION[#C,#F] = #A;
    }
  }   
}
